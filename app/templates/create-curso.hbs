{{outlet}}

 <div class="row body-createcurso">
   <h2 class="titulo">Crear Curso</h2>

    <form class="col s12 form-createcurso">

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input  id="first_name" type="text" class="validate">
          <label >Código de curso</label>
        </div>
        <div class="input-field col s6">
          <input id="last_name" type="text" class="validate">
          <label for="curso">Nombre de Curso</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="disabled" type="text" class="validate">
          <label for="disabled">Competencia Impactada</label>
        </div>
         <div class="input-field col s6">
          <input id="password" type="password" class="validate">
          <label for="password">Área Temática</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Fecha de inicio</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Fecha de fin</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Duración planeada (hrs)</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Duración real (hrs)</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Estatus</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Nueva Fecha</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Número de participantes convocados</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Número de participantes por área</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Origen</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Tipo de curso</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Centro de costos</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Costo planeado</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Costo real</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Costo prorrateado</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Tipo de evaluacion</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">LBT (responsable)</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <select>
            <option value="" disabled selected>Escoge un instructor</option>
            <option value="1">Option 1</option>
            <option value="2">Option 2</option>
            <option value="3">Option 3</option>
          </select>
          <label>Instructor</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Horas del instructor</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Pendientes</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Interno/Externo</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Áreas del curso</label>
        </div>
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Cantidad de cursos/grupo</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="email" type="email" class="validate">
          <label for="email">Tipo de evidencia</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="col s5">
          <div>Cafetería</div>
          <div class="switch">
            <label>
              No
              <input type="checkbox">
              <span class="lever"></span>
              Si
            </label>
          </div>
        </div>

        <div class="col s7">
          <div>White/Blue collar</div>
          <div class="switch">
            <label>
              WC
              <input type="checkbox">
              <span class="lever"></span>
              BC
            </label>
          </div>
        </div>
      </div>

      <button class="btn waves-effect waves-light" type="submit" name="action">Crear curso
        <i class="material-icons right">send</i>
      </button>
    </form>
  </div>
