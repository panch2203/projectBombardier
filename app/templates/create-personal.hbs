{{outlet}}
 <div class="row body-createcurso">
   <h3 class="">Crear personal</h3>

    <form class="col s12 form-createcurso">

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="matricula" type="text" class="validate">
          <label for="matricula">Matrícula</label>
        </div>
        <div class="input-field col s6">
          <input id="nombre" type="text" class="validate">
          <label for="nombre">Nombre de personal</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="curp" type="text" class="validate">
          <label for="curp">CURP</label>
        </div>
         <div class="input-field col s6">
          <input id="area" type="text" class="validate">
          <label for="area">Área</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="input-field col s6">
          <input id="puesto" type="text" class="validate">
          <label for="puesto">Puesto</label>
        </div>
        <div class="input-field col s6">
          <input id="userType" type="email" class="validate">
          <label for="userType">Tipo de usuario</label>
        </div>
      </div>

      <div class="row row-createcurso">
        <div class="col s7">
          <div>White/Blue collar</div>
          <div class="switch">
            <label>
              WC
              <input type="checkbox">
              <span class="lever"></span>
              BC
            </label>
          </div>
        </div>
      </div>

      <button class="btn waves-effect waves-light" type="submit" name="action">Crear curso
        <i class="material-icons right">send</i>
      </button>
    </form>
  </div>